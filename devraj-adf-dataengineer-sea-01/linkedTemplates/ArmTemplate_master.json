{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devraj-adf-dataengineer-sea-01"
		},
		"AzureDataLakeTempStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeTempStorage'"
		},
		"BronzeAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'BronzeAzureDataLakeStorage'"
		},
		"AzureDataLakeTempStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devrajprojstorageaccount.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0611-183948-8yowsddh"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devraj-keyvault-sea-01.vault.azure.net/"
		},
		"BronzeAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devrajprojstorageaccount.dfs.core.windows.net/"
		},
		"OnPremSqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DEVRAJPC\\SQLEXPRESS"
		},
		"OnPremSqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2017"
		},
		"OnPremSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mrk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devraj-adf-dataengineer-sea-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeTempStorage_accountKey": {
						"value": "[parameters('AzureDataLakeTempStorage_accountKey')]"
					},
					"BronzeAzureDataLakeStorage_accountKey": {
						"value": "[parameters('BronzeAzureDataLakeStorage_accountKey')]"
					},
					"AzureDataLakeTempStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeTempStorage_properties_typeProperties_url')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"BronzeAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('BronzeAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"OnPremSqlServer_properties_typeProperties_server": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_server')]"
					},
					"OnPremSqlServer_properties_typeProperties_database": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_database')]"
					},
					"OnPremSqlServer_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}